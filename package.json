{
    "name": "synthql-us-elections-finance",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "dotenvx": "dotenvx",
        "dev": "dotenvx run --verbose -- next dev",
        "build": "dotenvx run --verbose -- next build",
        "start": "dotenvx run --verbose -- next start",
        "lint": "dotenvx run --verbose -- next lint",
        "format": "dotenvx run --verbose -- prettier . --write",
        "create:db": "npx tsc src/create.ts --outDir ./dist && dotenvx run -- node dist/create",
        "insert:db": "npx tsc src/insert.ts --outDir ./dist && dotenvx run -- node dist/insert",
        "fetch:db": "npx tsc src/fetch.ts --outDir ./dist && dotenvx run -- node dist/fetch",
        "generate:db": "dotenvx run -- bash -c 'synthql generate --out=src/generated --defaultSchema=public --schemas public --url=\"$DATABASE_URL\"'"
    },
    "dependencies": {
        "@dotenvx/dotenvx": "^1.6.4",
        "@synthql/backend": "^0.78.4",
        "@synthql/cli": "^0.78.4",
        "@synthql/queries": "^0.78.4",
        "@synthql/react": "^0.78.4",
        "@tanstack/react-query": "^5.51.18",
        "clsx": "^2.1.1",
        "next": "14.2.5",
        "pg": "^8.12.0",
        "react": "^18",
        "react-dom": "^18",
        "tailwind-merge": "^2.4.0"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/pg": "^8.11.6",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "eslint": "^8",
        "eslint-config-next": "14.2.5",
        "eslint-config-prettier": "^9.1.0",
        "postcss": "^8",
        "prettier": "3.3.3",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    }
}
